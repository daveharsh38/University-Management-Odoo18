<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <!--    Report Function in Expense -->
    <record id="action_students_id_card" model="ir.actions.report">
        <field name="name">Student I-Card</field>
        <field name="model">university.students</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">university_management.university_students_i_card</field>
        <field name="report_file">university_management.university_students_i_card</field>
        <field name="print_report_name">'ID_%s' % (object.admission_number or object.name)</field>
        <field name="binding_model_id" ref="model_university_students"/>
        <field name="binding_type">report</field>
    </record>

</odoo>

